<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>TodoSummaryConfig_Base</class>
<comment>Configuration page for the Korganizer To-do Summary</comment>
<author>Allen Winter &lt;winter@kde.org&gt;</author>
<widget class="QWidget">
    <property name="name">
        <cstring>TodoSummaryConfig_Base</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>300</width>
            <height>277</height>
        </rect>
    </property>
    <property name="sizePolicy">
        <sizepolicy>
            <hsizetype>5</hsizetype>
            <vsizetype>5</vsizetype>
            <horstretch>0</horstretch>
            <verstretch>0</verstretch>
        </sizepolicy>
    </property>
    <property name="windowTitle">
        <string>To-do Summary Configuration</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QLayoutWidget" row="0" column="0">
            <property name="name">
                <cstring>layout19</cstring>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QButtonGroup">
                    <property name="name">
                        <cstring>mDaysGroup</cstring>
                    </property>
                    <property name="title">
                        <string>Show to-dos due:</string>
                    </property>
                    <vbox>
                        <property name="name">
                            <cstring>unnamed</cstring>
                        </property>
                        <widget class="QRadioButton">
                            <property name="name">
                                <cstring>mDateTodayButton</cstring>
                            </property>
                            <property name="text">
                                <string>&amp;Today only</string>
                            </property>
                            <property name="accel">
                                <string>Alt+T</string>
                            </property>
                            <property name="checked">
                                <bool>true</bool>
                            </property>
                        </widget>
                        <widget class="QRadioButton">
                            <property name="name">
                                <cstring>mDateMonthButton</cstring>
                            </property>
                            <property name="text">
                                <string>Within the next &amp;month (31 days)</string>
                            </property>
                            <property name="accel">
                                <string>Alt+M</string>
                            </property>
                            <property name="checked">
                                <bool>false</bool>
                            </property>
                        </widget>
                        <widget class="QLayoutWidget">
                            <property name="name">
                                <cstring>layout18</cstring>
                            </property>
                            <hbox>
                                <property name="name">
                                    <cstring>unnamed</cstring>
                                </property>
                                <widget class="QRadioButton">
                                    <property name="name">
                                        <cstring>mDateRangeButton</cstring>
                                    </property>
                                    <property name="enabled">
                                        <bool>true</bool>
                                    </property>
                                    <property name="text">
                                        <string>Within the &amp;next</string>
                                    </property>
                                    <property name="accel">
                                        <string>Alt+N</string>
                                    </property>
                                    <property name="checked">
                                        <bool>false</bool>
                                    </property>
                                </widget>
                                <widget class="QSpinBox">
                                    <property name="name">
                                        <cstring>mCustomDays</cstring>
                                    </property>
                                    <property name="enabled">
                                        <bool>false</bool>
                                    </property>
                                    <property name="suffix">
                                        <string> days</string>
                                    </property>
                                    <property name="maxValue">
                                        <number>365</number>
                                    </property>
                                    <property name="minValue">
                                        <number>1</number>
                                    </property>
                                    <property name="lineStep">
                                        <number>1</number>
                                    </property>
                                    <property name="value">
                                        <number>7</number>
                                    </property>
                                </widget>
                            </hbox>
                        </widget>
                    </vbox>
                </widget>
                <widget class="QButtonGroup">
                    <property name="name">
                        <cstring>mHideGroup</cstring>
                    </property>
                    <property name="title">
                        <string>Hide the following to-do types:</string>
                    </property>
                    <vbox>
                        <property name="name">
                            <cstring>unnamed</cstring>
                        </property>
                        <widget class="QCheckBox">
                            <property name="name">
                                <cstring>mHideCompletedBox</cstring>
                            </property>
                            <property name="acceptDrops">
                                <bool>true</bool>
                            </property>
                            <property name="text">
                                <string>&amp;Completed</string>
                            </property>
                            <property name="accel">
                                <string>Alt+C</string>
                            </property>
                            <property name="checked">
                                <bool>true</bool>
                            </property>
                        </widget>
                        <widget class="QCheckBox">
                            <property name="name">
                                <cstring>mHideOpenEndedBox</cstring>
                            </property>
                            <property name="text">
                                <string>&amp;Open-ended (no due date)</string>
                            </property>
                            <property name="accel">
                                <string>Alt+O</string>
                            </property>
                            <property name="checked">
                                <bool>false</bool>
                            </property>
                        </widget>
                        <widget class="QCheckBox">
                            <property name="name">
                                <cstring>mHideUnstartedBox</cstring>
                            </property>
                            <property name="text">
                                <string>&amp;Unstarted (start date is in the future)</string>
                            </property>
                            <property name="accel">
                                <string>Alt+U</string>
                            </property>
                        </widget>
                        <widget class="QCheckBox">
                            <property name="name">
                                <cstring>mHideInProgressBox</cstring>
                            </property>
                            <property name="text">
                                <string>&amp;In-progress (started but not completed)</string>
                            </property>
                            <property name="accel">
                                <string>Alt+I</string>
                            </property>
                        </widget>
                        <widget class="QCheckBox">
                            <property name="name">
                                <cstring>mHideOverdueBox</cstring>
                            </property>
                            <property name="text">
                                <string>Over&amp;due (not completed and beyond due-date)</string>
                            </property>
                            <property name="accel">
                                <string>Alt+D</string>
                            </property>
                        </widget>
                    </vbox>
                </widget>
            </vbox>
        </widget>
    </grid>
</widget>
<connections>
</connections>
<layoutdefaults spacing="6" margin="11"/>
</UI>
